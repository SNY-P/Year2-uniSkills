<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".taskActivity">

    <Button
        android:id="@+id/task_home_btn"
        android:layout_width="101dp"
        android:layout_height="84dp"
        android:text="Home"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="646dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/task_setting_btn"
        android:layout_width="102dp"
        android:layout_height="84dp"
        android:text="Setting"
        tools:layout_editor_absoluteX="308dp"
        tools:layout_editor_absoluteY="646dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/task_shop_btn"
        android:layout_width="101dp"
        android:layout_height="84dp"
        android:text="Shop"
        tools:layout_editor_absoluteX="206dp"
        tools:layout_editor_absoluteY="646dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/task_task_btn"
        android:layout_width="102dp"
        android:layout_height="84dp"
        android:text="Tasks"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="646dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Taks page"
        tools:layout_editor_absoluteX="167dp"
        tools:layout_editor_absoluteY="327dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>